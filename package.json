{
  "name": "itech-diamond-landing-page",
  "version": "1.0.0",
  "description": "Landing page para o e-commerce da Itech Diamond utilizando Tailwind CSS.",
  "main": "src/index.html",
  "scripts": {
    "build": "tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/output.css",
    "watch": "tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/output.css --watch",
    "start": "node src/server.js",
    "vercel-build": "npm run build",
    "dev": "nodemon src/server.js"
  },
  "author": "Lorenzo Sprenger",
  "license": "MIT",
  "dependencies": {
    "@vercel/analytics": "^1.4.1",
    "@vercel/speed-insights": "^1.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dompurify": "^2.5.8",
    "dotenv": "^10.0.0",
    "express": "^4.21.2",
    "express-rate-limit": "^6.0.0",
    "helmet": "^4.6.0",
    "live-server": "^1.2.1",
    "mysql2": "^2.3.3",
    "tailwindcss": "^2.2.19"
  },
  "keywords": [
    "e-commerce",
    "landing page",
    "tailwindcss",
    "itech diamond",
    "ferramentas",
    "suporte",
    "tecnologia",
    "usinagem",
    "usinagem de metais",
    "usinagem de aço",
    "usinagem de alumínio",
    "usinagem de cobre",
    "usinagem de aço inox"
  ],
  "devDependencies": {
    "nodemon": "^3.1.9"
  }
}
